<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
   
        
        <bean id="User" class="com.java.beans.User">
        
        <property name="unameString" value=""> </property>
        <property name="passString" value="Password"> </property>
        </bean>
        
        <bean id="UserValidatior" class="com.java.beans.UserValidator">
        	<property name="resource" value="/com/java/resources/Messages.properties"></property>
        </bean>
        
        
</beans>